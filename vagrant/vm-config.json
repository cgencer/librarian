        ]
    ],
    "packages_node_3": [
        "run_list": [
            "git",
            "tools",
            "custom"
        ]
    ],
    "defaults": {
        "git_user": {
            "email": "email",
            "username": "username",
            "private_key": "privateKey"
        },
        "postgresql": {
            "config": {
                "listen_addresses": "*",
                "port": "5432"
            },
            "pg_hba": [{
                "type": "local",
                "db": "postgres",
                "user": "postgres",
                "addr": "nil",
                "method": "trust"
            }, {
                "type": "host",
                "db": "all",
                "user": "all",
                "addr": "0.0.0.0/0",
                "method": "md5"
            }, {
                "type": "host",
                "db": "all",
                "user": "all",
                "addr": "::1/0",
                "method": "md5"
            }],
            "password": {
                "postgres": "super_secret"
            }
        },
        "redis": {
            "bind": "127.0.0.1",
            "port": "6379",
            "config_path": "/etc/redis/redis.conf",
            "daemonize": "yes",
            "timeout": "300",
            "loglevel": "notice"
        },
        "nginx": {
            "dir": "/etc/nginx",
            "log_dir": "/var/log/nginx",
            "binary": "/usr/sbin/nginx",
            "user": "www-data",
            "init_style": "runit",
            "pid": "/var/run/nginx.pid",
            "worker_connections": "1024"
      },
    },
    "ports": [{
        "ssh": {
            "node": -1,
            "protocol": "udp",
            "gst": 22,
            "hst": 2222
        }
    }, {
        "rdp": {
            "node": 0,
            "gst": 3389,
            "hst": 3389
        }        
    }, {
        "web": {
