{
    "packages": {
        "node_0": {
            "description": "This one is the main access point, holding our nginx reverse proxy / web server",
            "run_list": [
                "git",
                "nginx",
                "apache2",
                "tools",
                "custom"
            ],
            "defaults": [{
                "nginx": {
                    "dir": "/etc/nginx",
                    "log_dir": "/var/log/nginx",
                    "binary": "/usr/sbin/nginx",
                    "user": "www-data",
                    "init_style": "runit",
                    "pid": "/var/run/nginx.pid",
                    "worker_connections": "1024"
                }
            }]
        },
        "node_1": {
            "run_list": [
                "git",
                "nodejs",
                "postgres",
                "tools",
                "custom"
            ],
            "defaults": [{
                "git_user": {
                    "email": "email",
                    "username": "username",
                    "private_key": "privateKey"
                },
                "postgresql": {
                    "config": {
                        "listen_addresses": "*",
                        "port": "5432"
                    },
                    "pg_hba": [{
                        "type": "local",
                        "db": "postgres",
                        "user": "postgres",
                        "addr": "nil",
                        "method": "trust"
                    }, {
                        "type": "host",
                        "db": "all",
                        "user": "all",
                        "addr": "0.0.0.0/0",
                        "method": "md5"
                    }, {
                        "type": "host",
                        "db": "all",
                        "user": "all",
                        "addr": "::1/0",
                        "method": "md5"
                    }],
                    "password": {
                        "postgres": "super_secret"
                    }
                }
            }]
        },
        "node_2": {
            "run_list": [
                "git",
                "redis",
                "tools",
                "custom"
            ],
            "defaults": [{
                "redis": {
                    "bind": "127.0.0.1",
                    "port": "6379",
                    "config_path": "/etc/redis/redis.conf",
                    "daemonize": "yes",
                    "timeout": "300",
                    "loglevel": "notice"
                }
            }]
        },
        "node_3": {
            "run_list": [
                "git",
                "redis",
                "tools",
                "custom"
            ],
            "defaults": [{
                "redis": {
                    "bind": "127.0.0.1",
                    "port": "6379",
                    "config_path": "/etc/redis/redis.conf",
                    "daemonize": "yes",
                    "timeout": "300",
                    "loglevel": "notice"
                }
            }]
        }
    },
