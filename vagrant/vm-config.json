{
    "run_list": [
        "git",
        "nginx",
        "tools",
        "custom"
    ],
    "packages_node_0": [
        "run_list": [
            "git",
            "nginx",
            "tools",
            "custom"
        ]
    ],
    "packages_node_1": [
        "run_list": [
            "git",
            "tools",
            "custom"
        ]
    ],
    "packages_node_2": [
        "run_list": [
            "git",
            "tools",
